// Compare precompiled kernel with codegen generated for the AggColOp.

X = reshape( seq(-5.0, 6.0), 4, 3 );

// Double
print(sum(X, 1));

// Float
X = as.matrix<f32>(X);
print(sum(X, 1));

// Signed Integer
X = as.matrix<si64>(X);
print(sum(X, 1));

// Unsigned Integer
X = fill(2u, 4, 3);
print(sum(X, 1));